<launch>
    <env name="ROSCONSOLE_CONFIG_FILE" value="$(find next_best_view)/log/rosconsole.config" />
	<include file="$(find recognition_for_grasping)/launch/recognition_manager.launch" />
	<include file="$(find navigation_files)/launch/mild_p144_sim.launch" />
	<include file="$(find next_best_view)/launch/RobotModelService.launch" />
	<include file="$(find world_model)/launch/world_model.launch" />
	<node pkg="next_best_view" type="next_best_view" name="nbv" output="screen">
	  <rosparam file="$(find next_best_view)/launch/next_best_view_settings.yaml"/>
	  <rosparam file="$(find next_best_view)/launch/VisualizationConfig.yaml"/>
	</node>
</launch>